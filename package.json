{
  "name": "parse-midi",
  "version": "1.0.9",
  "description": "A small parser for MIDI messages.",
  "main": "dist/parseMidi.js",
  "type": "module",
  "files": [
    "dist/"
  ],
  "scripts": {
    "start": "npm run clean && npm run build:demo -- -w",
    "build": "npm run clean && npm run build:code && npm run build:demo",
    "build:code": "tsc",
    "build:demo": "tsc --module system --outFile dist/demoBundle.js",
    "clean": "rimraf dist",
    "lint": "tslint --format stylish --project .",
    "test": "jest",
    "prepublish": "npm test && npm run lint && npm run build"
  },
  "author": "Daniel Levett",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dlevs/parse-midi"
  },
  "devDependencies": {
    "@dlevs/tslint-config": "1.0.4",
    "@types/jest": "25.1.5",
    "jest": "25.2.7",
    "jest-ts-webcompat-resolver": "1.0.0",
    "rimraf": "2.6.2",
    "ts-jest": "25.3.1",
    "tslint": "6.1.1",
    "typescript": "3.4.1"
  }
}
